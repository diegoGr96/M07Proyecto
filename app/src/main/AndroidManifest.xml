<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.diego.m07proyecto">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logoexpressat"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MisTemas"
            android:parentActivityName=".MenuPrincipal"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".FilteredThemes"
            android:parentActivityName=".MenuPrincipal"
            android:screenOrientation="portrait"/>
        <activity android:name=".AddRespuesta"
            android:parentActivityName=".DetailActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Launcher"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Bienvenida3"
            android:screenOrientation="portrait"/>
        <activity android:name=".Bienvenida2"
            android:screenOrientation="portrait"/>
        <activity android:name=".Bienvenida"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".MenuPrincipal"
            android:label="@string/title_activity_menu_principal"
            android:theme="@style/NoActionBar"
            android:screenOrientation="portrait"/>
        <activity android:name=".PaginaPrincipalActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".RegistrarUsuarioActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.diego.m07proyecto.MainActivity" />
        </activity>
        <activity android:name=".DetailActivity"
            android:parentActivityName=".MenuPrincipal"
            android:screenOrientation="portrait"/>
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">

            <!--
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            -->
        </activity>
    </application>

</manifest>